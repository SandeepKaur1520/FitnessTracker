<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.fitness.tracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Body_Info"></activity>
        <activity
            android:name=".Tracking_info"
            android:label="@string/title_activity_tracking_info"></activity>
        <activity android:name=".Period_info" />
        <activity android:name=".Selected_Field" />
        <activity android:name=".Login_account" />
        <activity android:name=".Health_Info" />
        <activity
            android:name=".Personal_info"
            android:launchMode="singleTask" />
        <activity
            android:name=".Login_Screen"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />
        <activity android:name=".Welcome_screenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>